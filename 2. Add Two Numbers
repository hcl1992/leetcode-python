# Definition for singly-linked list.
#class ListNode(object):
 #    def __init__(self, x):
  #       self.val = x
   #      self.next = None

class Solution:
    # @return a ListNode
    def addTwoNumbers(self, l1, l2):
        if l1 == None:
            return l2
        if l2 == None:
            return l1
        answer = l1
        carry=(l1.val + l2.val) / 10;
        l1.val = (l1.val + l2.val) % 10;
        while l1.next != None and l2.next != None:
            temp = l1.next.val + l2.next.val + carry
            carry = temp // 10
            l1.next.val = temp % 10
            l1 = l1.next
            l2 = l2.next
        if l1.next == None:
            l1.next = l2.next
        while l1.next != None:
            temp = l1.next.val + carry
            l1.next.val = temp % 10;
            carry = temp // 10
            l1 = l1.next
        if carry>0:
            l1.next = ListNode(carry)
        return answer
